@using Pilot.BlazorClient.Service
@inject TokenAuthenticationStateProvider AuthenticationService
@inject NavigationManager NavigationManager

@code {

    protected override async Task OnInitializedAsync()
    {
        var token = await AuthenticationService.GetTokenAsync();
        if (token == null)
        {
            NavigationManager.NavigateTo("User/Login", forceLoad: true);
        }
    }

}